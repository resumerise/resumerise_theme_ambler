<!DOCTYPE html>
<html class="<%= it.type %>">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		<title><%= it.resume.basics?.name ? it.resume.basics.name : ''%></title>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
		<style>
			<%~ it.css %>
			<%~ it.widgetCss %>
		</style>
	</head>
	<body>

<table>
  <thead>
  <tr>
  <td>
    <div class="header-space">&nbsp;</div>
  </tr>
  </td>
  </thead>
  <tbody>
  <tr>
  <td>
    <div class="content">
	
	<main>
		<div class="header">
		<!-- BASICS -->
		<%~ include("basic", {
			resume: it.resume
		}) %> 
		</div>
		<div class="main-div">

		<div class="left-section--container">
		<!-- PROJECT -->
			<%~ include("project", {
				resume: it.resume
			}) %>
			<!-- WORK -->
			<%~ include("work", {
				resume: it.resume
			}) %>
			<!-- EDUCATION -->
			<%~ include("education", {
				resume: it.resume
			}) %>
			<!-- REFERENCE -->
			<%~ include("reference", {
				resume: it.resume
			}) %>
		</div>
		<div class="right-section--container">
			<!-- PUBLICATION -->
			<%~ include("publication", {
				resume: it.resume
			}) %>
				
			<!-- VOLUNTEER -->
			<%~ include("volunteer", {
				resume: it.resume
			}) %>
			
			<!-- CERTIFICATION -->
			<%~ include("certification", {
				resume: it.resume
			}) %>

			<!-- SKILLS -->
			<%~ include("skills", {
				resume: it.resume
			}) %>
			
			<!-- AWARDS -->
			<%~ include("awards", {
				resume: it.resume
			}) %>
			
			<!-- INTEREST -->
			<%~ include("interest", {
				resume: it.resume
			}) %>

			<!-- LANGUAGE -->
			<%~ include("language", {
				resume: it.resume
			}) %>

			<!-- PROFILE -->
			<%~ include("profile", {
				resume: it.resume
			}) %>
		</div>
			</div>

		</main>
	
	</div>
	</tr>
  </td>
  </tbody>
  <tfoot>
  <tr>
  <td>
    <div class="footer-space">&nbsp;</div>
	</tr>
  </td>
  </tfoot>
</table>

		<div class="footer">
		 <div class="contact--details--print">
        <%~ include('key-url-item', {
            label: it.resume.settings?.labels?.categoryProperty?.basics?.website, 
            value: it.resume.basics.website,
            classes: 'article__url--print',
            iconClass: 'fas fa-link'
        }) %>
        <%~ include('key-mail-item', {
            label: it.resume.settings?.labels?.categoryProperty?.basics?.email, 
            value: it.resume.basics.email,
            classes: 'article__mail--print',
            iconClass: 'far fa-envelope'
        }) %>
        <%~ include('key-value-item', {
            label: it.resume.settings?.labels?.categoryProperty?.basics?.phone, 
            value: it.resume.basics.phone,
            classes: 'article__phone--print',
            iconClass: 'fas fa-phone'
        }) %>
      
        </div>
		</div>
	</body>
</html>
